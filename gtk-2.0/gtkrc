style "flat" 		= "default"
{
	
	engine "pixmap"
	{
		image
		{
			function		= SHADOW
		}
	}
}

style "layout"	= "default"
{
	engine "pixmap"
	{
		image
		{
			function		= SHADOW
			detail			= "entry"
			shadow		= IN
			recolorable	= FALSE
			file			= "Shadows/text.svg"
			border		= { 1, 1, 1, 1 }
			stretch		= TRUE
		}
		image
		{
			function		= BOX
			detail		= "button"
			state			= NORMAL
			file			= "Buttons/button-normal.svg"
			recolorable	= TRUE
			border		= { 2, 3, 2, 3 }
			stretch		= TRUE
		}
	}
}

##################### STATUSBAR ###############################

style "statusbar"		= "default"
{

#	xthickness = 1
#	ythickness = 1
	
	engine "pixmap" 
	{
		image
		{
			function		= RESIZE_GRIP
			recolorable	= TRUE
			#state			= NORMAL
			detail		= "statusbar"
			overlay_file	= "Handles/resize-grip.svg"

			overlay_border	= {0,0,0,0 }
			overlay_stretch	= FALSE
		}
	}
}

##################### LISTHEADERS ###################x

style "list-header"
{
# 	Comment out the ythickness setting below for thicker column headers.
# 	ythickness = 0
	GtkTreeView::odd_row_color 	= "#26262a"
	GtkTreeView::even_row_color 	= "#1e1e21"

#  fg[NORMAL] = "#1e1e21"
#  text[NORMAL] = "#1e1e21"

	engine "pixmap" 
	{

# 		This image is used to draw the headers of columns in list views when they are
#		not selected.
		image
		{
			function        	= BOX
			recolorable    	= TRUE
			state				= NORMAL
			file            		= "ListHeaders/list_header.svg"
			border          	= { 2, 2, 2, 2 }
			stretch         	= TRUE
		}

#		This image is used to draw the column headers in list views when they are
#		clicked.
		image
		{
			function        	= BOX
			recolorable    	= TRUE
			state				= PRELIGHT
			file            		= "ListHeaders/list_header-prelight.svg"
			border          	= { 2, 2, 2, 2 }
			stretch         	= TRUE
		}

#		Does this do anything?

		image
		{
			function        	= BOX
			recolorable    	= TRUE
			state				= ACTIVE
			file					= "ListHeaders/list_header-pressed.svg"
			border          	= { 2, 2, 2, 2}
			stretch         	= TRUE
		}
		image
		{
			function        	= BOX
			recolorable    	= TRUE
			state				= SELECTED
			file					= "ListHeaders/list_header-prelight.svg"
			border          	= { 2, 2, 2, 2 }
			stretch         	= TRUE
		}
		image
		{
			function        	= BOX
			recolorable    	= TRUE
			state				= INSENSITIVE
			file					= "ListHeaders/list_header-insens.svg"
			border          	= { 2, 2, 2, 2 }
			stretch         	= TRUE
		}
	}
}

widget_class "*List" style "list-header"
widget_class "*GtkTree*" style "list-header"
widget_class "*GtkCList*" style "list-header"
#widget_class "*Tree*" style "list-header"


# This prevents Sodipodi from crashing while opening the
# Object-Style dialog.

style "unstyle"
{
	engine ""
	{	
	}
}

# recognizable pressed toggle buttons
# SPIcons seem to erase the background first. That's why I can't use
# the button style.

style "SPbutton"
{
	engine "pixmap"
	{
		image
		{
			function		= BOX
			shadow		= IN
			recolorable	= TRUE
			file			= "Shadows/shadow-out.svg"
			border		= { 2, 2, 2, 2 }
			stretch		= TRUE
		}
		image
		{
			function		= BOX
		}
	}
}

style "theme-etree" = "list-header"
{
   bg[ACTIVE]          = "#47494d"
}


style "chromeFix"
{

	ChromeGtkFrame::frame-color = "#000"
	ChromeGtkFrame::inactive-frame-color = "#000"

	bg[NORMAL]       	= "#1793d1" # Normal Background
	bg[SELECTED]     	= "#5bc8ff"
	

	text[NORMAL]	  	= "#1e1e21" # Text in window, arrows
	text[INSENSITIVE]	= "#fff" # Insensitive arrows
	text[SELECTED]   	= "#6c9eab" # Active text in active window

	engine "pixmap"
	{
		image
		{
			function			= BOX
			recolorable		= TRUE
			file				= "Menu-Menubar/menuitem.png"
			border			= { 10, 10, 10, 10 }
			stretch			= TRUE
		}
	}
}


style "ChromeMenuItemFix" = "menuitem"
{

	bg[NORMAL]       	= "#1793d1" # Normal Background
	bg[SELECTED]     	= "#5bc8ff"

	text[NORMAL]	  	= "#1e1e21" # Text in window, arrows
	text[INSENSITIVE]	= "#fff" # Insensitive arrows
	text[SELECTED]   	= "#6c9eab" # Active text in active window

}

style "chromeMenuFix" = "menu"{

	bg[NORMAL]       	= "#1793d1" # Normal Background
	bg_pixmap[NORMAL]	= "Menu-Menubar/menuitem.png"
	bg[SELECTED]     	= "#5bc8ff"

	text[NORMAL]	  	= "#1e1e21" # Text in window, arrows
	text[INSENSITIVE]	= "#3b97c1" # Insensitive arrows
	text[SELECTED]   	= "#6c9eab" # Active text in active window

}

