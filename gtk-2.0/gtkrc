  fg[SELECTED]     	= "#1793d1"
  fg[INSENSITIVE]  	= "#808080776c58"

  bg[NORMAL]       	= "#26262a" # Normal Background
  bg[ACTIVE]       	= "#1e1e21" # Mouseclicking, Tabs, active window list
  bg[PRELIGHT]     	= "#1e1e21" # Expand prelight bg
  bg[SELECTED]     	= "#262729"
  bg[INSENSITIVE]  	= "#1e1e21"

  base[NORMAL]     	= "#26262a" # Background, most
  base[ACTIVE]     	= "#1793d1" # Menu active item in inactive window
  base[PRELIGHT]   	= "#1e1e21"
  base[INSENSITIVE]	= "#1e1e21" # Inactive Entry bg
  base[SELECTED]   	= "#1793d1" # Menu active item in active window

  text[NORMAL]	  	= "#b6b6bd" # Text in window, arrows
  text[INSENSITIVE]	= "#b6b6bd" # Insensitive arrows
  text[SELECTED]   	= "#1e1e21" # Active text in active window
  text[ACTIVE]     	= "#b6b6bd" # Active text in inactive window
  text[PRELIGHT]   	= "#1e1e21" # Text on Mouseover

  engine "pixmap"
  {
    image
    {
      function				= HANDLE
